{
  "$schema": "https://schema.yext.com/config/answers/answers-config/v1",
  "$id": "blog-search",
  "name": "blog-search",
  "supportedLocales": [],
  "countryRestrictions": [],
  "verticals": {
    "blogs": {
      "entityTypes": [
        "ce_blog"
      ],
      "name": "Blogs",
      "searchableFields": {
        "builtin.entityType": {
          "nlpFilter": true
        },
        "datePosted": {
          "sortable": true
        },
        "name": {
          "textSearch": true
        }
      },
      "sortBys": [
        {
          "buckets": [],
          "direction": "DESC",
          "field": "datePosted",
          "type": "FIELD"
        }
      ],
      "source": "KNOWLEDGE_MANAGER"
    }
  },
  "localizations": {},
  "additionalSearchablePlaces": [],
  "querySuggestions": {
    "disablePopularQueries": false,
    "verticalPromptsOnEmpty": false,
    "popularQueriesBlacklistedTerms": [],
    "universalPrompts": [],
    "verticalPrompts": {
      "blogs": [
        "[[name]]"
      ]
    }
  }
}